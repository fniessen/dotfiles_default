#!/usr/bin/env bash

rm -rf -v ~/.cache/mozilla
rm -rf -v ~/.emacs.d/auctex-auto-generated-info/*
rm -rf -v ~/.emacs.d/auto-save-list/.*
rm -rf -v ~/.emacs.d/backups/*
rm -rf -v ~/.emacs.d/url/*
rm -rf -v ~/.gnus.d/cache/nnimap*
rm -rf -v ~/.dotfiles/lib/received-emails
rm -rf -v ~/.dotfiles/plugins/oh-my-zsh/cache/*
rm -rf -v ~/.local/*

rm -v ~/.emacs.d/projectile.cache
# rm -v ~/.histfile
rm -v ~/.lesshst

find ~/ -iname 'nul' | xargs rm -rf -v
find ~/ -iname 'null' | xargs rm -rf -v
find ~/ -iname 'MDB_ODBC.Trace.Txt' | xargs rm -rf -v
# find ~/ -iname '*.exe.stackdump' | xargs rm -rf -v
